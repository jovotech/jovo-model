{
  "type": "object",
  "properties": {
    "entityTypes": {
      "type": "object",
      "patternProperties": {
        ".*": {
          "$ref": "#/definitions/EntityType"
        }
      },
      "additionalProperties": false
    },
    "dialogflow": {
      "$ref": "#/definitions/DialogflowModel"
    },
    "intents": {
      "type": "object",
      "patternProperties": {
        ".*": {
          "$ref": "#/definitions/Intent"
        }
      },
      "additionalProperties": false
    },
    "invocation": {
      "anyOf": [
        {
          "$ref": "#/definitions/InvocationObject"
        },
        {
          "type": "string"
        }
      ]
    }
  },
  "required": ["invocation"],
  "definitions": {
    "EntityType": {
      "type": "object",
      "properties": {
        "dialogflow": {
          "anyOf": [
            {
              "type": "object",
              "properties": {},
              "additionalProperties": true
            },
            {
              "type": "string"
            }
          ]
        },
        "values": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/EntityTypeValue"
          }
        }
      }
    },
    "EntityTypeValue": {
      "type": "object",
      "properties": {
        "value": {
          "type": "string"
        },
        "id": {
          "type": "string"
        },
        "key": {
          "type": "string"
        },
        "synonyms": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": ["value"]
    },
    "DialogflowModel": {
      "type": "object",
      "properties": {
        "intents": {
          "type": "array",
          "items": [
            {
              "$ref": "#/definitions/DialogflowLMEntityObject"
            }
          ],
          "minItems": 1,
          "additionalItems": {
            "anyOf": [
              {
                "$ref": "#/definitions/DialogflowLMEntityObject"
              }
            ]
          }
        }
      }
    },
    "DialogflowLMEntityObject": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "auto": {
          "type": "boolean"
        },
        "webhookUsed": {
          "type": "boolean"
        },
        "fallbackIntent": {
          "type": "boolean"
        },
        "responses": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/DialogflowResponse"
          }
        },
        "events": {
          "type": "array",
          "items": [
            {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                }
              },
              "required": ["name"]
            }
          ],
          "minItems": 1,
          "additionalItems": {
            "anyOf": [
              {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  }
                },
                "required": ["name"]
              }
            ]
          }
        }
      },
      "required": ["auto", "name", "webhookUsed"]
    },
    "DialogflowResponse": {
      "type": "object",
      "properties": {
        "parameters": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/DialogflowLMEntityParameterObject"
          }
        }
      },
      "required": ["parameters"]
    },
    "DialogflowLMEntityParameterObject": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "isList": {
          "type": "boolean"
        },
        "value": {
          "type": "string"
        },
        "dataType": {
          "type": "string"
        }
      },
      "required": ["dataType", "isList", "name", "value"]
    },
    "Intent": {
      "type": "object",
      "properties": {
        "phrases": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "samples": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "entities": {
          "type": "object",
          "patternProperties": {
            ".*": {
              "$ref": "#/definitions/IntentEntity"
            }
          },
          "additionalProperties": false
        }
      }
    },
    "IntentEntity": {
      "type": "object",
      "properties": {
        "text": {
          "type": "string"
        },
        "type": {
          "anyOf": [
            {
              "$ref": "#/definitions/Record<string,string>"
            },
            {
              "type": "string"
            }
          ]
        }
      }
    },
    "Record<string,string>": {
      "type": "object"
    },
    "InvocationObject": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}

