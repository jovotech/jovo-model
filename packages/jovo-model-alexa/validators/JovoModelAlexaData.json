{
  "type": "object",
  "properties": {
    "alexa": {
      "$ref": "#/definitions/AlexaModel"
    },
    "inputTypes": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/InputType"
      }
    },
    "entityTypes": {
      "type": "object",
      "patternProperties": {
        ".*": {
          "$ref": "#/definitions/EntityType"
        }
      },
      "additionalProperties": false
    },
    "intents": {
      "anyOf": [
        {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Intent"
          }
        },
        {
          "type": "object",
          "patternProperties": {
            ".*": {
              "$ref": "#/definitions/Intent"
            }
          },
          "additionalProperties": false
        }
      ]
    },
    "invocation": {
      "anyOf": [
        {
          "type": "object",
          "properties": {},
          "additionalProperties": true
        },
        {
          "type": "string"
        }
      ]
    }
  },
  "required": ["invocation"],
  "definitions": {
    "AlexaModel": {
      "type": "object",
      "properties": {
        "interactionModel": {
          "type": "object",
          "properties": {
            "languageModel": {
              "type": "object",
              "properties": {
                "invocationName": {
                  "type": "string"
                },
                "intents": {
                  "type": "array",
                  "items": {
                    "$ref": "#/definitions/AlexaLMIntent"
                  }
                },
                "types": {
                  "type": "array",
                  "items": {
                    "$ref": "#/definitions/AlexaLMTypeObject"
                  }
                }
              }
            }
          },
          "required": ["languageModel"]
        }
      },
      "required": ["interactionModel"]
    },
    "AlexaLMIntent": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "slots": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/AlexaLMInputObject"
          }
        },
        "samples": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": ["name"]
    },
    "AlexaLMInputObject": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "type": {
          "anyOf": [
            {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            },
            {
              "type": "string"
            }
          ]
        },
        "alexaInputObjtype": {
          "type": "string"
        }
      },
      "required": ["name", "type"]
    },
    "AlexaLMTypeObject": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "values": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/AlexaLMTypeValue"
          }
        }
      },
      "required": ["name", "values"]
    },
    "AlexaLMTypeValue": {
      "type": "object",
      "properties": {
        "id": {
          "type": ["null", "string"]
        },
        "name": {
          "type": "object",
          "properties": {
            "value": {
              "type": "string"
            },
            "synonyms": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "required": ["value"]
        }
      },
      "required": ["id", "name"]
    },
    "InputType": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "values": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/EntityTypeValue"
          }
        }
      },
      "required": ["name"]
    },
    "EntityType": {
      "type": "object",
      "properties": {
        "values": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/EntityTypeValue"
          }
        }
      }
    },
    "EntityTypeValue": {
      "type": "object",
      "properties": {
        "value": {
          "type": "string"
        },
        "id": {
          "type": "string"
        },
        "key": {
          "type": "string"
        },
        "synonyms": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": ["value"]
    },
    "Intent": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "phrases": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "samples": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "inputs": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/IntentInput"
          }
        },
        "entities": {
          "type": "object",
          "patternProperties": {
            ".*": {
              "$ref": "#/definitions/IntentEntity"
            }
          },
          "additionalProperties": false
        }
      }
    },
    "IntentInput": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "text": {
          "type": "string"
        },
        "type": {
          "anyOf": [
            {
              "$ref": "#/definitions/Record<string,string>"
            },
            {
              "type": "string"
            }
          ]
        }
      },
      "required": ["name"]
    },
    "IntentEntity": {
      "type": "object",
      "properties": {
        "text": {
          "type": "string"
        },
        "type": {
          "anyOf": [
            {
              "$ref": "#/definitions/Record<string,string>"
            },
            {
              "type": "string"
            }
          ]
        }
      }
    },
    "Record<string,string>": {
      "type": "object"
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}
